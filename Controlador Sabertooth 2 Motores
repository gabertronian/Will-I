int motor1=10;
int motor2=9;
byte motorcontrol= 80; 
byte motorcontro2=80;
void setup() {
  // put your setup code here, to run once:
  
  Serial.begin(9600);
  pinMode(motor1,OUTPUT);
  pinMode(motor2,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  motorcontrol = 1;
  Serial.write(motorcontrol);
  motorcontro2=1;
  Serial.write(motorcontro2);

}
